<table class='table'>
	<thead>
		<tr>
			<th scope='col'>#</th>
			<th scope='col'>First Name</th>
			<th scope='col'>Last Name</th>
			<th scope='col'>Enail</th>
			<th scope='col'>Phone</th>
			<th scope='col'>Actions</th>
		</tr>
	</thead>
	<tbody>
		{{#each contacts}}
			<tr>
				<th scope='row'>{{@index}}</th>
				<td>{{firstname}}</td>
				<td>{{lastname}}</td>
				<td>{{email}}</td>
				<td>{{phone}}</td>
				<td></td>
			</tr>
		{{/each}}
	</tbody>
</table>